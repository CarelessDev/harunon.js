{
  "name": "harunon.js",
  "version": "3.0.0-beta.2",
  "description": "Multipurpose Discord Bot with Music that serves our server",
  "main": "index.js",
  "author": "CarelessDev",
  "license": "MIT",
  "type": "module",
  "repository": "https://github.com/CarelessDev/harunon.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/*",
    "cb": "yarn clean && yarn build",
    "start": "node --es-module-specifier-resolution=node dist/bot.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src && prettier --check \"src/**/*.ts\""
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.8.0",
    "chalk": "^5.0.0",
    "cocoa-discord-utils": "^1.1.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "google-tts-api": "^2.0.2",
    "libsodium-wrappers": "^0.7.9",
    "node-fetch": "^3.2.1",
    "ytdl-core": "^4.11.0"
  }
}
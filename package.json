    "eslint": "^8.9.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "cocoa-discord-utils": "^1.0.0-rc.4",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "node-fetch": "^3.2.0"
  }

    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@leomotors/music-bot": "^0.6.0",
    "@yimura/scraper": "^1.2.3",
    "chalk": "^5.0.1",
    "dotenv": "^16.0.1",
    "google-tts-api": "^2.0.2",
